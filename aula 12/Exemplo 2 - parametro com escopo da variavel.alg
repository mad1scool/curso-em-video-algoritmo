algoritmo "semnome"
// Função :
// Autor :
// Data : 11/01/2022
// Seção de Declarações 
var
   x, y: inteiro
   procedimento soma(A, B: inteiro)
   inicio
         escreval("Valor A: ",A)
         escreval("Valor B: ",B)
         escreval("A soma entre os dois é ", A+B)
   fimprocedimento
   
inicio
      x <- 5
      y <- 3
      soma(x, y)
fimalgoritmo
