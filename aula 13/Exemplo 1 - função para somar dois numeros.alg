algoritmo "semnome"
// Função :
// Autor :
// Data : 11/01/2022
// Seção de Declarações 
var
   num1, num2, r: inteiro
   
funcao soma(a, b: inteiro) : inteiro
inicio
   retorne a+b
fimfuncao

inicio
      escreva("Numero 1: ")
      leia(num1)
      escreva("Numero 2: ")
      leia(num2)
      r <- soma(num1, num2)
      escreva(r)


fimalgoritmo
